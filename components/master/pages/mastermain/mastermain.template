  <element type="codeElement"><![CDATA[
var globalcount = 1;

function showlogin()
{
  // we show login
  WA.$N('mastermain|single|modalbackground').show();
  WA.$N('mastermain|single|modallogin').show();
//  WA.Managers.wa4gl.resizeApplication('mastermain', 'single');
}

function hidelogin()
{
  // we hide login
  WA.$N('mastermain|single|modalbackground').hide();
  WA.$N('mastermain|single|modallogin').hide();
}

function startmodal(app, params, feedback)
{
  // we show login
  WA.$N('mastermain|single|modalbackground').show();
  WA.$N('mastermain|single|generalmodal').show();
  // load the application and set feedback
  var xapp = app.split('|');
  WA.Managers.wa4gl.startApplication('mastermain|single|contentmodal', xapp[0], xapp[1], params, '');
}

function stopmodal()
{
  // we hide login
  WA.$N('mastermain|single|modalbackground').hide();
  WA.$N('mastermain|single|generalmodal').hide();
}

function startcalendar()
{
  WA.$N('mastermain|single|calendarmodal').show();
}

function stopcalendar()
{
  WA.$N('mastermain|single|calendarmodal').hide();
}

function startloading()
{
  // we show login
  WA.$N('mastermain|single|loadingbackgroundmodal').show();
}

function stoploading()
{
  // we hide login
  WA.$N('mastermain|single|loadingbackgroundmodal').hide();
}

function addzero(num)
{
  if (num < 10)
    return '0' + num;
  return num;
}

function timer()
{
  var fecha=new Date();
  var dias = ["Domingo", "Lunes", "Martes", "Miércoles", "Jueves", "Viernes", "Sábado"];
  var meses = ["Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio", "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre"];

  WA.browser.setInnerHTML(WA.toDOM('mastermain|single|timer'), dias[fecha.getDay()] + ', ' + fecha.getDate() + ' de ' + meses[fecha.getMonth()] + ' de ' + fecha.getFullYear() + ', ' + addzero(fecha.getHours()) + ':' + addzero(fecha.getMinutes()) + ':' + addzero(fecha.getSeconds()));
  setTimeout( function() { timer(); }, 1000 );
}

function alerts(request)
{
  var data = JSONDecode(request.responseText);
  var strmsg = '';
  if (data.messages > 0)
    strmsg += data.messages + ' mensaje' + (data.messages>1?'s':'');
  var stralert = '';
  if (data.alerts > 0)
    stralert += data.alerts + ' alerta' + (data.alerts>1?'s':'');
    if (strmsg || stralert)
  {
    WA.browser.setInnerHTML(WA.toDOM('mastermain|single|alerts'), 'Tienes: ' + strmsg + (strmsg&&stralert?' y ':'') + stralert + '.');
  }
  else
    WA.browser.setInnerHTML($('mastermain|single|alerts'), '');
}

var userkey = __USERKEY__;
var alertsr = null;
function getalerts()
{
  if (!userkey)
  {
    WA.browser.setInnerHTML($('mastermain|single|alerts'), '');
    return;
  }
  alertsr = WA.Managers.ajax.createPeriodicRequest(10000, 100000000, '/status/alerts.'+userkey+'.json', 'post', null, alerts, true);
  alertsr.addStateFeedback(function() {}, 0); // ignore errors
}

function setSound(status)
{
  WA.Managers.sound.on = status;
  if (status)
    WA.toDOM('mastermain|single|sound_header').className = 'buttonmenusoundon';
  else
    WA.toDOM('mastermain|single|sound_header').className = 'buttonmenusoundoff';
}

function setHelp(status)
{
  switch (status)
  {
    case 0:
      WA.Managers.help.switchOff();
      WA.toDOM('mastermain|single|help_header').className = 'buttonmenuhelpmute';
      break;
    case 1:
      WA.Managers.help.switchAlt();
      WA.toDOM('mastermain|single|help_header').className = 'buttonmenuhelp';
      break;
    case 2:
      WA.Managers.help.switchOn();
      WA.toDOM('mastermain|single|help_header').className = 'buttonmenuhelpfull';
      break;
  }
}

function debugconsole()
{
  var node = WA.$N('masterindex|single|app_console');
  if (!node)
